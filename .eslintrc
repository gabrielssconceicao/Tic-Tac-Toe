{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [

    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off",
      "react/no-array-index-key": "off",
      "jsx-a11y/click-events-have-key-events": "off"
    }
}
